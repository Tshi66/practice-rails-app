<div class="container">
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-8">
      <h1>ユーザーの一覧</h1>
      <p>ユーザー検索ができます</p>
      <%= form_tag(users_path, :method => 'get' ) do %>
        <%= text_field_tag :search %>
        <%= submit_tag 'Search', :name => nil %>
      <% end %>
      <%= render 'user' %>
    </div>
  </div>
</div>